{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "basic": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "basic",
        "stream": "ext://sys.stdout"
      },
      "debug_file_handler": {
        "class": "logging.handlers.RotatingFileHandler",
        "level": "DEBUG",
        "formatter": "basic",
        "filename": "data/log/debug.log",
        "maxBytes": 10485760,
        "backupCount": 20,
        "encoding": "utf8"
      }
    },
    "loggers": {
      "core": {
        "level": "DEBUG",
        "handlers": [
          "console",
          "debug_file_handler"
        ],
        "propagate": "no"
      },
      "urllib3": {
        "level": "ERROR",
        "handlers": [],
        "propagate": "no"
      },
      "httpx": {
        "level": "ERROR",
        "handlers": [],
        "propagate": "no"
      },
      "openai": {
        "level": "ERROR",
        "handlers": [],
        "propagate": "no"
      }
    },
    "root": {
      "level": "INFO",
      "handlers": [
        "console"
      ]
    }
  }